        TextBox.Parent = Frame
        TextBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextBox.BackgroundTransparency = 0.650
        TextBox.BorderColor3 = Color3.fromRGB(0, 255, 0)
