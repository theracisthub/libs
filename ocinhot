    function NormalElements:NewSlider(text, tip, minVal, maxVal, callback)
        local Updates = {}
        local Frame = Instance.new("Frame")
        local Frame_2 = Instance.new("Frame")
        local TextLabel = Instance.new("TextLabel")
        local TextLabel_2 = Instance.new("TextLabel")
        local Tip = Instance.new("ImageButton")
        local TextButton = Instance.new("TextButton")

        Frame.Parent = ScrollingFrame
        Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Frame.BackgroundTransparency = 0.750
        Frame.BorderColor3 = Color3.fromRGB(0, 255, 0)
        Frame.Position = UDim2.new(-0.56807512, 0, -0.00212765951, 0)
        Frame.Size = UDim2.new(0, 213, 0, 38)
        
        TextLabel.Parent = Frame
        TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.BackgroundTransparency = 1.000
        TextLabel.Position = UDim2.new(0.0281690136, 0, 0.157894731, 0)
        TextLabel.Size = UDim2.new(0, 86, 0, 11)
        TextLabel.Font = Enum.Font.Gotham
        TextLabel.Text = " "..tostring(text)
        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.TextSize = 16.000
        TextLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        Tip.Name = "Icon"
        Tip.Parent = Frame
        Tip.BackgroundTransparency = 1.000
        Tip.Position = UDim2.new(0.869000018, 0, 0.256000012, 0)
        Tip.Size = UDim2.new(0, 20, 0, 20)
        Tip.Image = "rbxassetid://7072978559"
        
        TextLabel_2.Parent = Frame
        TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel_2.BackgroundTransparency = 1.000
        TextLabel_2.Position = UDim2.new(0.464788735, 0, 0.157894731, 0)
        TextLabel_2.Size = UDim2.new(0, 86, 0, 11)
        TextLabel_2.Font = Enum.Font.Gotham
        TextLabel_2.Text = tostring(minVal)
        TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel_2.TextSize = 16.000
        TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left
        
        TextButton.Parent = Frame
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.650
        TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextButton.Position = UDim2.new(0.023, 0, 0.550000012, 0)
        TextButton.Size = UDim2.new(0, 171, 0, 14)
        TextButton.Font = Enum.Font.SourceSans
        TextButton.Text = ""
        TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.TextSize = 14.000
        
        Frame_2.Parent = TextButton
        Frame_2.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        Frame_2.BorderColor3 = Color3.fromRGB(0, 255, 0)
        Frame_2.Size = UDim2.new(0, 0, 0, 14)
        Frame_2.BackgroundTransparency = 0.750

        Tip.MouseButton1Click:Connect(function()
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Tip!";
                Text = tip;
                Duration = 7;
            })
        end)

        callback = callback or function() end
        local mouse = game.Players.LocalPlayer:GetMouse()
        local uis = game:GetService("UserInputService")
        local Value = 0
        local StartConnection, StopConnection
        
        TextButton.MouseButton1Down:Connect(function()
            Value = math.floor((((tonumber(maxVal) - tonumber(minVal)) / 171) * Frame_2.AbsoluteSize.X) + tonumber(minVal)) or 0
            pcall(function()
                callback(Value)
                TextLabel_2.Text = tostring(Value)
            end)
            Frame_2.Size = UDim2.new(0, math.clamp(mouse.X - Frame_2.AbsolutePosition.X, 0, 171), 0, 14)
            StartConnection = mouse.Move:Connect(function()
                Value = math.floor((((tonumber(maxVal) - tonumber(minVal)) / 171) * Frame_2.AbsoluteSize.X) + tonumber(minVal))
                pcall(function()
                    callback(Value)
                    TextLabel_2.Text = tostring(Value)
                end)
                Frame_2.Size = UDim2.new(0, math.clamp(mouse.X - Frame_2.AbsolutePosition.X, 0, 171), 0, 14)
            end)
            StopConnection = uis.InputEnded:Connect(function(Mouse)
                if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                    Value = math.floor((((tonumber(maxVal) - tonumber(minVal)) / 171) * Frame_2.AbsoluteSize.X) + tonumber(minVal))
                    pcall(function()
                        callback(Value)
                        TextLabel_2.Text = tostring(Value)
                    end)
                    Frame_2.Size = UDim2.new(0, math.clamp(mouse.X - Frame_2.AbsolutePosition.X, 0, 171), 0, 14)
                    StartConnection:Disconnect()
                    StopConnection:Disconnect()
                end
            end)
        end)

        function Updates:UpdateMinVal(newVal)
            minVal = newVal
        end
        function Updates:UpdateMaxVal(newVal)
            maxVal = newVal
        end

        return Updates
    
    end

    function NormalElements:NewGuiToggle(text)
        local TextLabel = Instance.new("TextLabel")
        local TextButton = Instance.new("TextButton")

        TextLabel.Parent = ScrollingFrame
        TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextLabel.BackgroundTransparency = 0.750
        TextLabel.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextLabel.Size = UDim2.new(0, 213, 0, 38)
        TextLabel.Font = Enum.Font.Gotham
        TextLabel.Text = "  "..tostring(text)
        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.TextSize = 16.000
        TextLabel.TextXAlignment = Enum.TextXAlignment.Left

        TextButton.Parent = TextLabel
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.750
        TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextButton.Position = UDim2.new(0.652676165, 0, 0.21026291, 0)
        TextButton.Size = UDim2.new(0, 65, 0, 22)
        TextButton.Font = Enum.Font.Gotham
        TextButton.Text = 'RightControl'
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 16.000
        TextButton.TextScaled = true
        TextButton.TextWrapped = true

        local ToggleKey = "RightControl"
        local AwaitForClick = false
        TextButton.MouseButton1Click:Connect(function()
            if AwaitForClick == false then
                TextButton.Text = "..."
                AwaitForClick = true
            end
        end)

        game:GetService("UserInputService").InputBegan:Connect(function(a,b)
            if a.UserInputType == Enum.UserInputType.Keyboard then
                if a.KeyCode.Name:lower() == ToggleKey:lower() then
                    Main.Visible = not Main.Visible
                end
                if AwaitForClick == true then
                    TextButton.Text = a.KeyCode.Name
                    ToggleKey = a.KeyCode.Name
                    AwaitForClick = false
                end
            end
        end)
    end

    function NormalElements:NewDropDown(text, tble, callback)
        tble = tble or {}
        local Updates = {}
        local TextButton = Instance.new("TextButton")
        local TextLabel = Instance.new("TextLabel")
        local TextLabel_2 = Instance.new("TextLabel")
        local Icon = Instance.new("ImageLabel")
        local DropDownFrame = Instance.new("ScrollingFrame")
        local CancelButton = Instance.new("TextButton")
        local UIListLayout = Instance.new("UIListLayout")

        TextButton.Parent = ScrollingFrame
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.750
        TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextButton.Position = UDim2.new(-0.56807512, 0, -0.00212765951, 0)
        TextButton.Size = UDim2.new(0, 213, 0, 38)
        TextButton.Text = ""

        TextLabel.Parent = TextButton
        TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.BackgroundTransparency = 1.000
        TextLabel.Position = UDim2.new(0.0281690136, 0, 0.157894731, 0)
        TextLabel.Size = UDim2.new(0, 86, 0, 11)
        TextLabel.Font = Enum.Font.Gotham
        TextLabel.Text = " "..tostring(text)
        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.TextSize = 16.000
        TextLabel.TextXAlignment = Enum.TextXAlignment.Left

        TextLabel_2.Parent = TextButton
        TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel_2.BackgroundTransparency = 1.000
        TextLabel_2.Position = UDim2.new(0.0280000009, 0, 0.589999974, 0)
        TextLabel_2.Size = UDim2.new(0, 86, 0, 11)
        TextLabel_2.Font = Enum.Font.Gotham
        TextLabel_2.Text = " None"
        TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel_2.TextSize = 16.000
        TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left

        Icon.Name = "Icon"
        Icon.Parent = TextButton
        Icon.BackgroundTransparency = 1.000
        Icon.Position = UDim2.new(0.868544579, 0, 0.255813956, 0)
        Icon.Rotation = 90.000
        Icon.Size = UDim2.new(0, 20, 0, 20)
        Icon.Image = "rbxassetid://7072706745"

        DropDownFrame.Parent = Main
        DropDownFrame.Active = true
        DropDownFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        DropDownFrame.BackgroundTransparency = 1.000
        DropDownFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
        DropDownFrame.Position = UDim2.new(0, 0, 0.187234044, 0)
        DropDownFrame.Size = UDim2.new(0, 213, 0, 191)
        DropDownFrame.ScrollBarThickness = 0
        DropDownFrame.Visible = false
        DropDownFrame.Name = "pagebruv"

        UIListLayout.Parent = DropDownFrame
        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout.Padding = UDim.new(0, 1)
        
        CancelButton.Parent = DropDownFrame
        CancelButton.Name = "CancelMe"
        CancelButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        CancelButton.BackgroundTransparency = 0.500
        CancelButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        CancelButton.Size = UDim2.new(0, 213, 0, 38)
        CancelButton.Font = Enum.Font.Gotham
        CancelButton.Text = "  Cancel"
        CancelButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        CancelButton.TextSize = 16.000
        CancelButton.TextXAlignment = Enum.TextXAlignment.Left

        TextButton.MouseButton1Click:Connect(function()
            for i,v in pairs(Main:GetChildren()) do
                if v:IsA("ScrollingFrame") and v.Name:lower() == "pagebruv" then
                    v.Visible = false
                end
            end
            DropDownFrame.Visible = true
        end)

        DropDownFrame.ChildAdded:Connect(function(a)
            repeat wait(0.001) until a.Parent ~= nil
            local cur_size = UIListLayout.AbsoluteContentSize
            DropDownFrame.CanvasSize = UDim2.new(0,cur_size.X,0,cur_size.Y)
        end)
    
        DropDownFrame.ChildRemoved:Connect(function(a)
            local cur_size = UIListLayout.AbsoluteContentSize
            DropDownFrame.CanvasSize = UDim2.new(0,cur_size.X,0,cur_size.Y)
        end)

        for i, v in pairs(tble) do
            local TextButton = Instance.new("TextButton")
            TextButton.Parent = DropDownFrame
            TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            TextButton.BackgroundTransparency = 0.750
            TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
            TextButton.Size = UDim2.new(0, 213, 0, 38)
            TextButton.Font = Enum.Font.Gotham
            TextButton.Text = "  "..tostring(v)
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextSize = 16.000
            TextButton.TextXAlignment = Enum.TextXAlignment.Left 
            TextButton.MouseButton1Click:Connect(function()
                TextLabel_2.Text = " "..tostring(v)
                DropDownFrame.Visible = false
                ScrollingFrame.Visible = true
                callback(v)
            end)
        end

        CancelButton.MouseButton1Click:Connect(function()
            for i,v in pairs(Main:GetChildren()) do
                if v:IsA("ScrollingFrame") and v.Name:lower() == "pagebruv" then
                    v.Visible = false
                end
            end
            ScrollingFrame.Visible = true
        end)
        
        function Updates:UpdateList(newList)
            for i,v in pairs(DropDownFrame:GetChildren()) do
                if v.Name ~= 'CancelMe' and v:IsA("TextButton") then
                    v:Destroy()
                end
            end
            tble = newList or {}
            for i, v in pairs(tble) do
                local TextButton = Instance.new("TextButton")
                TextButton.Parent = DropDownFrame
                TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                TextButton.BackgroundTransparency = 0.750
                TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
                TextButton.Size = UDim2.new(0, 213, 0, 38)
                TextButton.Font = Enum.Font.Gotham
                TextButton.Text = "  "..tostring(v)
                TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextButton.TextSize = 16.000
                TextButton.TextXAlignment = Enum.TextXAlignment.Left 
                TextButton.MouseButton1Click:Connect(function()
                    TextLabel_2.Text = " "..tostring(v)
                    DropDownFrame.Visible = false
                    ScrollingFrame.Visible = true
                    callback(v)
                end)
            end
        end

        return Updates
    end

    function NormalElements:NewKeyBind(text, default_key, callback)
        local TextLabel = Instance.new("TextLabel")
        local TextButton = Instance.new("TextButton")
        local DefaultKey = default_key or Enum.KeyCode.C

        TextLabel.Parent = ScrollingFrame
        TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextLabel.BackgroundTransparency = 0.750
        TextLabel.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextLabel.Size = UDim2.new(0, 213, 0, 38)
        TextLabel.Font = Enum.Font.Gotham
        TextLabel.Text = "  "..tostring(text)
        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.TextSize = 16.000
        TextLabel.TextXAlignment = Enum.TextXAlignment.Left

        TextButton.Parent = TextLabel
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.750
        TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextButton.Position = UDim2.new(0.652676165, 0, 0.21026291, 0)
        TextButton.Size = UDim2.new(0, 65, 0, 22)
        TextButton.Font = Enum.Font.Gotham
        TextButton.Text = DefaultKey.Name
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 16.000
        TextButton.TextScaled = true
        TextButton.TextWrapped = true

        local AwaitForClick = false
        game:GetService("UserInputService").InputBegan:Connect(function(a,b)
            if a.UserInputType == Enum.UserInputType.Keyboard then
                if AwaitForClick == true then
                    TextButton.Text = a.KeyCode.Name
                    DefaultKey = a.KeyCode
                    AwaitForClick = false
                end
                if a.KeyCode.Name:lower() == DefaultKey.Name:lower() then
                    callback(true)
                end
            end
        end)
        game:GetService("UserInputService").InputEnded:Connect(function(a,b)
            if a.UserInputType == Enum.UserInputType.Keyboard then
                if a.KeyCode.Name:lower() == DefaultKey.Name:lower() then
                    callback(false)
                end
            end
        end)

        TextButton.MouseButton1Click:Connect(function()
            if AwaitForClick == false then
                TextButton.Text = "..."
                AwaitForClick = true
            end
        end)
    end

