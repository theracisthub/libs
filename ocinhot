local Ocin = {}

function Ocin:Create(title)
    local ScreenGui = Instance.new("ScreenGui")
    local YourMom = Instance.new("Frame")
    local Header = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    local MainFrame = Instance.new("Frame")
    local SideBar = Instance.new("Frame")
    local Frame = Instance.new("Frame")
    local UIListLayout = Instance.new("UIListLayout")

    ScreenGui.Parent = game:service"CoreGui"
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    YourMom.Name = "Ocin"
    YourMom.Parent = ScreenGui
    YourMom.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    YourMom.BackgroundTransparency = 1.000
    YourMom.Position = UDim2.new(0.38691932, 0, 0.352848113, 0)
    YourMom.Size = UDim2.new(0, 569, 0, 325)
    YourMom.Active = true
    YourMom.Draggable = true

    Header.Name = "Header"
    Header.Parent = YourMom
    Header.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Header.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Header.Size = UDim2.new(0, 569, 0, 21)

    TextLabel.Parent = Header
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1.000
    TextLabel.Size = UDim2.new(0, 225, 0, 21)
    TextLabel.Font = Enum.Font.Gotham
    TextLabel.Text = " "..tostring(title)
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.TextSize = 20.000
    TextLabel.TextXAlignment = Enum.TextXAlignment.Left

    MainFrame.Name = "MainFrame"
    MainFrame.Parent = YourMom
    MainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    MainFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    MainFrame.Position = UDim2.new(0, 0, 0.0892307684, 0)
    MainFrame.Size = UDim2.new(0, 569, 0, 296)

    SideBar.Name = "SideBar"
    SideBar.Parent = MainFrame
    SideBar.BackgroundColor3 = Color3.fromRGB(7, 7, 7)
    SideBar.BorderColor3 = Color3.fromRGB(7, 7, 7)
    SideBar.Size = UDim2.new(0, 151, 0, 296)

    Frame.Parent = SideBar
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.BackgroundTransparency = 1.000
    Frame.Position = UDim2.new(0.0397351012, 0, 0.0202702694, 0)
    Frame.Size = UDim2.new(0, 138, 0, 284)

    UIListLayout.Parent = Frame
    UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 5)

    local Elements = {}
    local DeployedAlready = false

    function Elements:ToggleUI()
        YourMom.Visible = not YourMom.Visible
    end

    function Elements:CreateHome(text)
        local TextButton = Instance.new("TextButton")
        local HomeArea = Instance.new("Frame")
        local pfp =  Instance.new("ImageLabel")
        local TextLabel = Instance.new("TextLabel")
        local UICorner = Instance.new("UICorner")

        HomeArea.Name = "HomeArea"
        HomeArea.Parent = MainFrame
        HomeArea.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        HomeArea.BackgroundTransparency = 1.000
        HomeArea.Position = UDim2.new(0.284999996, 0, 0.0270000007, 0)
        HomeArea.Size = UDim2.new(0, 397, 0, 280)
        HomeArea.Visible = false
        
        pfp.Name = "pfp"
        pfp.Parent = HomeArea
        pfp.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        pfp.BackgroundTransparency = 1.000
        pfp.Position = UDim2.new(0.372795969, 0, 0.0571428575, 0)
        pfp.Size = UDim2.new(0, 100, 0, 100)
        pfp.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
        
        UICorner.CornerRadius = UDim.new(0, 3)
        UICorner.Parent = pfp
        
        TextLabel.Parent = HomeArea
        TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.BackgroundTransparency = 1.000
        TextLabel.Position = UDim2.new(0.246851385, 0, 0.442857146, 0)
        TextLabel.Size = UDim2.new(0, 200, 0, 22)
        TextLabel.Font = Enum.Font.GothamMedium
        TextLabel.Text = "Welcome, "..tostring(game:service"Players".LocalPlayer.Name)
        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.TextSize = 16.000

        TextButton.Parent = Frame
        TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        if DeployedAlready == false then
            DeployedAlready = true
            HomeArea.Visible = true
            TextButton.BackgroundTransparency = 0.700
        else
            HomeArea.Visible = false
            TextButton.BackgroundTransparency = 0.950
        end
        TextButton.Size = UDim2.new(0, 140, 0, 24)
        TextButton.Font = Enum.Font.GothamMedium
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 16.000
        TextButton.Text = text

        local UICorner = Instance.new("UICorner")
        UICorner.CornerRadius = UDim.new(0, 3)
        UICorner.Parent = TextButton

        TextButton.MouseButton1Click:Connect(function()
            for i,v in pairs(MainFrame:GetChildren()) do
                if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                    v.Visible = false
                end
            end
            for i,v in pairs(SideBar.Frame:GetChildren()) do
                if v:IsA("TextButton") then
                    v.BackgroundTransparency = 0.950
                end
            end
            TextButton.BackgroundTransparency = 0.700
            HomeArea.Visible = true
        end)
        spawn(function()
            local c, err = game:GetService('Players'):GetUserThumbnailAsync(game:service"Players".LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
            if c then
                pfp.Image = c
            end
        end)
    end
    
    function Elements:NewTab(text)
        local TextButton = Instance.new("TextButton")
        local UICorner = Instance.new("UICorner")

        local ScrollingFrame = Instance.new("ScrollingFrame")
        local UIListLayout = Instance.new("UIListLayout")

        ScrollingFrame.Parent = MainFrame
        ScrollingFrame.Active = true
        ScrollingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        ScrollingFrame.BackgroundTransparency = 1.000
        ScrollingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
        ScrollingFrame.Position = UDim2.new(0.28471002, 0, 0.0270270277, 0)
        ScrollingFrame.Size = UDim2.new(0, 397, 0, 280)
        ScrollingFrame.ScrollBarThickness = 0
        
        UIListLayout.Parent = ScrollingFrame
        UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout.Padding = UDim.new(0, 4)

        TextButton.Parent = Frame
        TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        if DeployedAlready == false then
            DeployedAlready = true
            TextButton.BackgroundTransparency = 0.700
            ScrollingFrame.Visible = true
        else
            TextButton.BackgroundTransparency = 0.950
            ScrollingFrame.Visible = false
        end
        TextButton.Size = UDim2.new(0, 140, 0, 24)
        TextButton.Font = Enum.Font.GothamMedium
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 16.000
        TextButton.Text = text
        
        UICorner.CornerRadius = UDim.new(0, 3)
        UICorner.Parent = TextButton

        ScrollingFrame.ChildAdded:Connect(function(a)
            repeat wait(0.001) until a.Parent ~= nil
            local cur_size = UIListLayout.AbsoluteContentSize
            ScrollingFrame.CanvasSize = UDim2.new(0,cur_size.X,0,cur_size.Y)
        end)
    
        ScrollingFrame.ChildRemoved:Connect(function(a)
            local cur_size = UIListLayout.AbsoluteContentSize
            ScrollingFrame.CanvasSize = UDim2.new(0,cur_size.X,0,cur_size.Y)
        end)

        TextButton.MouseButton1Click:Connect(function()
            for i,v in pairs(MainFrame:GetChildren()) do
                if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                    v.Visible = false
                end
            end
            for i,v in pairs(SideBar.Frame:GetChildren()) do
                if v:IsA("TextButton") then
                    v.BackgroundTransparency = 0.950
                end
            end
            TextButton.BackgroundTransparency = 0.700
            ScrollingFrame.Visible = true
        end)

        local Sections = {}

        function Sections:NewSection(text)
            local TextLabel = Instance.new("TextLabel")
            local UICorner = Instance.new("UICorner")

            TextLabel.Parent = ScrollingFrame
            TextLabel.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
            TextLabel.BorderColor3 = Color3.fromRGB(34, 34, 34)
            TextLabel.Size = UDim2.new(0, 392, 0, 24)
            TextLabel.Font = Enum.Font.GothamMedium
            TextLabel.Text = tostring(text)
            TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.TextSize = 16.000

            UICorner.CornerRadius = UDim.new(0, 5)
            UICorner.Parent = TextLabel
            
            local InnerThings = {}

            function InnerThings:NewLabel(text)
                local TextLabel = Instance.new("TextLabel")
                local UICorner = Instance.new("UICorner")

                TextLabel.Parent = ScrollingFrame
                TextLabel.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                TextLabel.BorderColor3 = Color3.fromRGB(61, 61, 61)
                TextLabel.Size = UDim2.new(0, 392, 0, 24)
                TextLabel.Font = Enum.Font.GothamMedium
                TextLabel.Text = "  "..tostring(text)
                TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.TextSize = 16.000
                TextLabel.TextXAlignment = Enum.TextXAlignment.Left

                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = TextLabel
            end

            function InnerThings:NewButton(text, callback)
                local TextButton = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")

                TextButton.Parent = ScrollingFrame
                TextButton.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                TextButton.BorderColor3 = Color3.fromRGB(61, 61, 61)
                TextButton.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                TextButton.Size = UDim2.new(0, 392, 0, 24)
                TextButton.Font = Enum.Font.GothamMedium
                TextButton.Text = "  "..tostring(text)
                TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextButton.TextSize = 16.000
                TextButton.TextXAlignment = Enum.TextXAlignment.Left

                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = TextButton

                TextButton.MouseButton1Click:Connect(callback)
            end

            function InnerThings:NewToggle(text, callback)
                local State = false
                local TextButton = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")

                TextButton.Parent = ScrollingFrame
                TextButton.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                TextButton.BorderColor3 = Color3.fromRGB(61, 61, 61)
                TextButton.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                TextButton.Size = UDim2.new(0, 392, 0, 24)
                TextButton.Font = Enum.Font.GothamMedium
                TextButton.Text = "  "..tostring(text)
                TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextButton.TextSize = 16.000
                TextButton.TextXAlignment = Enum.TextXAlignment.Left
                
                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = TextButton
                
                local Frame = Instance.new("Frame")
                local UICorner = Instance.new("UICorner")

                Frame.Parent = TextButton
                Frame.BackgroundColor3 = Color3.fromRGB(255, 74, 74)
                Frame.Position = UDim2.new(0.944000006, 0, 0.0829999968, 0)
                Frame.Size = UDim2.new(0, 20, 0, 20)
                
                UICorner.CornerRadius = UDim.new(0, 100)
                UICorner.Parent = Frame

                local Red = Color3.fromRGB(255, 26, 26)
                local Green = Color3.fromRGB(0, 255, 29)
                TextButton.MouseButton1Click:Connect(function()
                    State = not State
                    if State then
                        Frame.BackgroundColor3 = Green
                    else
                        Frame.BackgroundColor3 = Red
                    end
                    callback(State)
                end)
            end

            function InnerThings:NewTextBox(text, preset, callback)
                preset = preset or "Input"
                local TextLabel = Instance.new("TextLabel")
                local UICorner = Instance.new("UICorner")

                TextLabel.Parent = ScrollingFrame
                TextLabel.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                TextLabel.BorderColor3 = Color3.fromRGB(61, 61, 61)
                TextLabel.Size = UDim2.new(0, 392, 0, 24)
                TextLabel.Font = Enum.Font.GothamMedium
                TextLabel.Text = "  "..tostring(text)
                TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.TextSize = 16.000
                TextLabel.TextXAlignment = Enum.TextXAlignment.Left
                
                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = TextLabel

                local TextBox = Instance.new("TextBox")
                local UICorner = Instance.new("UICorner")
                
                TextBox.Parent = TextLabel
                TextBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
                TextBox.BorderColor3 = Color3.fromRGB(40, 40, 40)
                TextBox.Position = UDim2.new(0.707591772, 0, 0.125, 0)
                TextBox.Size = UDim2.new(0, 110, 0, 18)
                TextBox.Font = Enum.Font.GothamMedium
                TextBox.PlaceholderColor3 = Color3.fromRGB(178, 178, 178)
                TextBox.Text = preset
                TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextBox.TextScaled = true
                TextBox.TextSize = 14.000
                TextBox.TextWrapped = true
                
                UICorner.CornerRadius = UDim.new(0, 3)
                UICorner.Parent = TextBox

                TextBox.FocusLost:Connect(function()
                    pcall(function() callback(TextBox.Text) end)
                end)
            end

            function InnerThings:NewSlider(text, _min_, _max_, callback)
                local TextLabel = Instance.new("TextLabel")
                local UICorner = Instance.new("UICorner")

                TextLabel.Parent = ScrollingFrame
                TextLabel.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                TextLabel.BorderColor3 = Color3.fromRGB(61, 61, 61)
                TextLabel.Size = UDim2.new(0, 392, 0, 24)
                TextLabel.Font = Enum.Font.GothamMedium
                TextLabel.Text = "  "..tostring(text)
                TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.TextSize = 16.000
                TextLabel.TextXAlignment = Enum.TextXAlignment.Left
                
                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = TextLabel

                local TextButton = Instance.new("TextButton")
                local Frame = Instance.new("Frame")
                local Num = Instance.new("TextLabel")
                
                TextButton.Parent = TextLabel
                TextButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
                TextButton.BorderColor3 = Color3.fromRGB(40, 40, 40)
                TextButton.Position = UDim2.new(0.61500001, 0, 0.375, 0)
                TextButton.Size = UDim2.new(0, 141, 0, 5)
                TextButton.Font = Enum.Font.SourceSans
                TextButton.Text = ""
                TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                TextButton.TextSize = 1.000
                
                Frame.Parent = TextButton
                Frame.BackgroundColor3 = Color3.fromRGB(16, 16, 16)
                Frame.BorderColor3 = Color3.fromRGB(16, 16, 16)
                Frame.Position = UDim2.new(-0.00222496944, 0, -0.0250000004, 0)
                Frame.Size = UDim2.new(0, 0, 0, 5)
                
                local UICorner = Instance.new("UICorner")
                UICorner.CornerRadius = UDim.new(0, 18)
                UICorner.Parent = Frame
                
                local UICorner = Instance.new("UICorner")
                UICorner.CornerRadius = UDim.new(0, 18)
                UICorner.Parent = TextButton
                
                Num.Parent = TextLabel
                Num.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Num.BackgroundTransparency = 1.000
                Num.BorderColor3 = Color3.fromRGB(27, 42, 53)
                Num.Position = UDim2.new(0.53061223, 0, 0.0416666567, 0)
                Num.Size = UDim2.new(0, 23, 0, 19)
                Num.Font = Enum.Font.GothamMedium
                Num.Text = "0"
                Num.TextColor3 = Color3.fromRGB(255, 255, 255)
                Num.TextSize = 16.000
                Num.TextXAlignment = Enum.TextXAlignment.Right

                local Value = 0
                local minvalue = _min_
                local maxvalue = _max_
                local mouse = game:service"Players".LocalPlayer:GetMouse()
                local Touching, Release = nil, nil
                TextButton.MouseButton1Down:Connect(function()
                    Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 141) * Frame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                    pcall(function()
                        callback(Value)
                    end)
                    Frame.Size = UDim2.new(0, math.clamp(mouse.X - Frame.AbsolutePosition.X, 0, 141), 0, 5)
                    Touching = mouse.Move:Connect(function()
                        Num.Text = tostring(Value)
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 141) * Frame.AbsoluteSize.X) + tonumber(minvalue))
                        pcall(function()
                            callback(Value)
                        end)
                        Frame.Size = UDim2.new(0, math.clamp(mouse.X - Frame.AbsolutePosition.X, 0, 141), 0, 5)
                    end)
                    Release = game:service"UserInputService".InputEnded:Connect(function(Mouse)
                        if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 141) * Frame.AbsoluteSize.X) + tonumber(minvalue))
                            pcall(function()
                                callback(Value)
                            end)
                            Frame.Size = UDim2.new(0, math.clamp(mouse.X - Frame.AbsolutePosition.X, 0, 141), 0, 5)
                            Touching:Disconnect()
                            Release:Disconnect()
                        end
                    end)
                end)
            end

            function InnerThings:NewDropdown(text, dropdownitems, callback)
                dropdownitems = dropdownitems or {}
                local OldText = text

                local TextButton = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")

                TextButton.Parent = ScrollingFrame
                TextButton.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                TextButton.BorderColor3 = Color3.fromRGB(61, 61, 61)
                TextButton.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                TextButton.Size = UDim2.new(0, 392, 0, 24)
                TextButton.Font = Enum.Font.GothamMedium
                TextButton.Text = "  "..tostring(text)
                TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextButton.TextSize = 16.000
                TextButton.TextXAlignment = Enum.TextXAlignment.Left
                
                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = TextButton

                local ItemFrame = Instance.new("ScrollingFrame")
                local UIListLayout = Instance.new("UIListLayout")
        
                ItemFrame.Parent = MainFrame
                ItemFrame.Active = true
                ItemFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                ItemFrame.BackgroundTransparency = 1.000
                ItemFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
                ItemFrame.Position = UDim2.new(0.28471002, 0, 0.0270270277, 0)
                ItemFrame.Size = UDim2.new(0, 397, 0, 280)
                ItemFrame.ScrollBarThickness = 0
                ItemFrame.Visible = false

                UIListLayout.Parent = ItemFrame
                UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout.Padding = UDim.new(0, 4)

                local ITEM = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")

                ITEM.Parent = ItemFrame
                ITEM.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                ITEM.BorderColor3 = Color3.fromRGB(61, 61, 61)
                ITEM.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                ITEM.Size = UDim2.new(0, 392, 0, 24)
                ITEM.Font = Enum.Font.GothamMedium
                ITEM.Text = "  Go back"
                ITEM.TextColor3 = Color3.fromRGB(255, 255, 255)
                ITEM.TextSize = 16.000
                ITEM.TextXAlignment = Enum.TextXAlignment.Left

                UICorner.CornerRadius = UDim.new(0, 5)
                UICorner.Parent = ITEM

                ITEM.MouseButton1Click:Connect(function()
                    for i,v in pairs(MainFrame:GetChildren()) do
                        if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                            v.Visible = false
                        end
                    end
                    ScrollingFrame.Visible = true
                end)

                for _, v in pairs(dropdownitems) do
                    local ITEM = Instance.new("TextButton")
                    local UICorner = Instance.new("UICorner")
    
                    ITEM.Parent = ItemFrame
                    ITEM.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                    ITEM.BorderColor3 = Color3.fromRGB(61, 61, 61)
                    ITEM.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                    ITEM.Size = UDim2.new(0, 392, 0, 24)
                    ITEM.Font = Enum.Font.GothamMedium
                    ITEM.Text = "  "..tostring(v)
                    ITEM.TextColor3 = Color3.fromRGB(255, 255, 255)
                    ITEM.TextSize = 16.000
                    ITEM.TextXAlignment = Enum.TextXAlignment.Left
    
                    UICorner.CornerRadius = UDim.new(0, 5)
                    UICorner.Parent = ITEM
    
                    ITEM.MouseButton1Click:Connect(function()
                        for i,v in pairs(MainFrame:GetChildren()) do
                            if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                                v.Visible = false
                            end
                        end
                        ScrollingFrame.Visible = true
                        TextButton.Text = "  "..tostring(OldText).." | "..tostring(ITEM.Text:gsub("  ",""))
                    end)
                end

                local Updates = {}

                function Updates:UpdateList(newlist)
                    newlist = newlist or {}

                    for i,v in pairs(ItemFrame:GetChildren()) do
                        if v:IsA("TextButton") then
                            v:Destroy()
                        end
                    end
                    local ITEM = Instance.new("TextButton")
                    local UICorner = Instance.new("UICorner")
    
                    ITEM.Parent = ItemFrame
                    ITEM.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                    ITEM.BorderColor3 = Color3.fromRGB(61, 61, 61)
                    ITEM.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                    ITEM.Size = UDim2.new(0, 392, 0, 24)
                    ITEM.Font = Enum.Font.GothamMedium
                    ITEM.Text = "  Go back"
                    ITEM.TextColor3 = Color3.fromRGB(255, 255, 255)
                    ITEM.TextSize = 16.000
                    ITEM.TextXAlignment = Enum.TextXAlignment.Left
    
                    UICorner.CornerRadius = UDim.new(0, 5)
                    UICorner.Parent = ITEM
    
                    ITEM.MouseButton1Click:Connect(function()
                        for i,v in pairs(MainFrame:GetChildren()) do
                            if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                                v.Visible = false
                            end
                        end
                        ScrollingFrame.Visible = true
                    end)
    
                    for _, v in pairs(newlist) do
                        local ITEM = Instance.new("TextButton")
                        local UICorner = Instance.new("UICorner")
        
                        ITEM.Parent = ItemFrame
                        ITEM.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                        ITEM.BorderColor3 = Color3.fromRGB(61, 61, 61)
                        ITEM.Position = UDim2.new(-0.0037783375, 0, 0, 0)
                        ITEM.Size = UDim2.new(0, 392, 0, 24)
                        ITEM.Font = Enum.Font.GothamMedium
                        ITEM.Text = "  "..tostring(v)
                        ITEM.TextColor3 = Color3.fromRGB(255, 255, 255)
                        ITEM.TextSize = 16.000
                        ITEM.TextXAlignment = Enum.TextXAlignment.Left
        
                        UICorner.CornerRadius = UDim.new(0, 5)
                        UICorner.Parent = ITEM
        
                        ITEM.MouseButton1Click:Connect(function()
                            for i,v in pairs(MainFrame:GetChildren()) do
                                if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                                    v.Visible = false
                                end
                            end
                            ScrollingFrame.Visible = true
                            TextButton.Text = "  "..tostring(OldText).." | "..tostring(ITEM.Text:gsub("  ",""))
                        end)
                    end
                end

                TextButton.MouseButton1Click:Connect(function()
                    for i,v in pairs(MainFrame:GetChildren()) do
                        if v:IsA("ScrollingFrame") or v.Name == "HomeArea" then
                            v.Visible = false
                        end
                    end
                    ItemFrame.Visible = true
                end)

                return Updates
            end

            return InnerThings
        end

        return Sections
    end

    return Elements
end

return Ocin
