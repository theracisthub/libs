        Tip.Position = UDim2.new(0.868544579, 0, 0.255813956, 0)
        Tip.Size = UDim2.new(0, 20, 0, 20)
        Tip.Image = "rbxassetid://7072978559"

        Tip.MouseButton1Click:Connect(function()
            game:GetService("StarterGui"):SetCore("SendNotification",{
