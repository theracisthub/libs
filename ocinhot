    UIListLayout.Parent = ScrollingFrame
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 1)

    ScrollingFrame.ChildAdded:Connect(function(a)
        repeat wait(0.001) until a.Parent ~= nil
        local cur_size = UIListLayout.AbsoluteContentSize
        ScrollingFrame.CanvasSize = UDim2.new(0,cur_size.X,0,cur_size.Y)
    end)

    ScrollingFrame.ChildRemoved:Connect(function(a)
        local cur_size = UIListLayout.AbsoluteContentSize
        ScrollingFrame.CanvasSize = UDim2.new(0,cur_size.X,0,cur_size.Y)
    end)

    local ToggleCache = {}
    local NormalElements = {}
    local Tabs = {}

    function NormalElements:NewButton(text, tip, callback)
        local Updates = {}
        local TextButton = Instance.new("TextButton")
        local Tip = Instance.new("ImageButton")

        TextButton.Parent = ScrollingFrame
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.750
        TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextButton.Size = UDim2.new(0, 213, 0, 38)
        TextButton.Font = Enum.Font.Gotham
        TextButton.Text = "  "..tostring(text)
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 16.000
        TextButton.TextXAlignment = Enum.TextXAlignment.Left

        Tip.Name = "Icon"
        Tip.Parent = TextButton
        Tip.BackgroundTransparency = 1.000
        Tip.Position = UDim2.new(0.868544579, 0, 0.255813956, 0)
        Tip.Size = UDim2.new(0, 20, 0, 20)
        Tip.Image = "rbxassetid://7072978559"

        Tip.MouseButton1Click:Connect(function()
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Tip!";
                Text = tip;
                Duration = 7;
            })
        end)

        TextButton.MouseButton1Click:Connect(callback)

        function Updates:Update(text)
            TextButton.Text = "  "..tostring(text)
        end

        return Updates
    end

    function NormalElements:NewToggle(text, tip, callback)
        local TextButton = Instance.new("TextButton")
        local Tip = Instance.new("ImageButton")
        local toggle = false
        local OffColor, OnColor = Color3.fromRGB(0, 0, 0), Color3.fromRGB(91, 91, 91)

        TextButton.Parent = ScrollingFrame
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.750
        TextButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
        TextButton.Size = UDim2.new(0, 213, 0, 38)
        TextButton.Font = Enum.Font.Gotham
        TextButton.Text = "  "..tostring(text)
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 16.000
        TextButton.TextXAlignment = Enum.TextXAlignment.Left

        Tip.Name = "Icon"
        Tip.Parent = TextButton
        Tip.BackgroundTransparency = 1.000
        Tip.Position = UDim2.new(0.868544579, 0, 0.255813956, 0)
        Tip.Size = UDim2.new(0, 20, 0, 20)
        Tip.Image = "rbxassetid://7072978559"

        Tip.MouseButton1Click:Connect(function()
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Tip!";
                Text = tip;
                Duration = 7;
            })
        end)

        TextButton.MouseButton1Click:Connect(function()
            toggle = not toggle
            if toggle then
                game:GetService("TweenService"):Create(TextButton,TweenInfo.new(0.15,Enum.EasingStyle.Linear, Enum.EasingDirection.In), {BackgroundColor3=OnColor}):Play()
            else
                game:GetService("TweenService"):Create(TextButton,TweenInfo.new(0.15,Enum.EasingStyle.Linear, Enum.EasingDirection.In), {BackgroundColor3=OffColor}):Play()
            end
            callback(toggle)
        end)
    end

    function NormalElements:NewTextBox(text, tip, callback)
        local Frame = Instance.new("Frame")
        local TextLabel = Instance.new("TextLabel")
        local Tip = Instance.new("ImageButton")
        local TextBox = Instance.new("TextBox")

        Frame.Parent = ScrollingFrame
