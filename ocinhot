local function FunctionsLib()
    local functions = {}
    function functions.CreateString()
        local res = ""
        for i = 1, math.random(10,15) do
            res = res .. string.char(math.random(97, 122))
        end
        return res
    end
    return functions
end

local Ocin = {}

function Ocin:Create(title)
    local ScreenGui = Instance.new("ScreenGui")
    local Main = Instance.new("Frame")
    local Boarder_2 = Instance.new("Frame")
    local Boarder_2_2 = Instance.new("Frame")
    local Boarder_3 = Instance.new("Frame")
    local Boarder_4 = Instance.new("Frame")
    local Boarder_5 = Instance.new("Frame")
    local SubBoarder_1 = Instance.new("Frame")
    local Title = Instance.new("TextLabel")
    local ScrollingFrame = Instance.new("ScrollingFrame")
    local UIListLayout = Instance.new("UIListLayout")

    local functions = FunctionsLib()

    ScreenGui.Parent = game.CoreGui
    ScreenGui.Name = functions.CreateString()
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    Main.Name = "Main"
    Main.Parent = ScreenGui
    Main.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Main.BackgroundTransparency = 0.550
    Main.Position = UDim2.new(0.45228219, 0, 0.42278859, 0)
    Main.Size = UDim2.new(0, 213, 0, 235)
    Main.Active = true
    Main.Draggable = true
    
    Boarder_2.Name = "Boarder_2"
    Boarder_2.Parent = Main
    Boarder_2.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    Boarder_2.BorderColor3 = Color3.fromRGB(0, 255, 0)
    Boarder_2.Position = UDim2.new(0, 0, 0, -1)
    Boarder_2.Size = UDim2.new(0, 213, 0, 0)
    
    Boarder_2_2.Name = "Boarder_2"
    Boarder_2_2.Parent = Main
